#open the apptainer
apptainer shell braker3.sif
#each run needs
#a_cytherea
braker.pl --AUGUSTUS_CONFIG_PATH=/mmfs1/gscratch/zaneveld/Augustus_config --genome=a_cytherea_full_mask_soft.fasta --prot_seq=../Metazoa.fa --threads=20
#copy out the breaker.aa, breaker.codingseq, and breaker.gtf and add a_cytherea to the biginning
#zip the output
